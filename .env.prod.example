# === Projeto01 — .env de Produção (EXEMPLO) ===
APP_ENV=prod
LOG_LEVEL=INFO

# --- Postgres (container db)
POSTGRES_DB=appdb
POSTGRES_USER=appuser
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# --- SQLAlchemy (usa rede interna do compose: host=db)
DATABASE_URL=postgresql+psycopg://appuser:CHANGE_ME_STRONG_PASSWORD@db:5432/appdb

# --- JWT (OBRIGATÓRIO em prod — >= 32 chars)
JWT_SECRET=CHANGE_ME_SUPER_STRONG_SECRET_32_CHARS_MIN
JWT_ALG=HS256
JWT_EXPIRES_MIN=60

# --- CORS (NÃO usar * em prod)
# Exemplo: https://seu-dominio.com,https://admin.seu-dominio.com
CORS_ORIGINS=https://SEU_DOMINIO

# --- Seed (opcional)
SEED_ON_STARTUP=false
SEED_ADMIN_EMAIL=admin@SEU_DOMINIO
SEED_ADMIN_PASSWORD=CHANGE_ME_STRONG_ADMIN_PASSWORD
SEED_SOURCE_NAME=partner_a
SEED_SOURCE_API_KEY=CHANGE_ME_PARTNER_A_KEY
